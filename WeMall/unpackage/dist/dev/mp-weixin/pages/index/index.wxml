<view><view class="status" style="{{'position:'+(headerPosition)+';'}}"></view><view class="header" style="{{'position:'+(headerPosition)+';'}}"><view class="input"><view class="icon search"></view><input disabled="{{true}}" placeholder="点击搜索" data-event-opts="{{[['tap',[['toSearch']]]]}}" bindtap="__e"/></view><view class="menu"><image mode="widthFix" src="{{userImage}}" data-event-opts="{{[['tap',[['profileClick',['$event']]]]]}}" bindtap="__e"></image></view></view><view class="place"></view><view class="swiper-view"><swiper class="swiper" indicator-dots="true" autoplay="true" circular="true" indicator-active-color="#ffffff"><block wx:for="{{swiperList}}" wx:for-item="swiper" wx:for-index="__i0__" wx:key="sid"><swiper-item data-event-opts="{{[['tap',[['toSwiper',['$0'],[[['swiperList','sid',swiper.sid]]]]]]]}}" bindtap="__e"><image mode="aspectFill" src="{{swiper.img}}"></image></swiper-item></block></swiper><view class="keep-out"></view></view><view class="category"><view class="box"><swiper class="swiper" style="{{'height:'+(categoryHeight)+';'}}" duration="300" data-event-opts="{{[['change',[['categoryChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{categoryList}}" wx:for-item="page" wx:for-index="pageindex" wx:key="pageindex"><swiper-item><view class="category-list"><block wx:for="{{page}}" wx:for-item="category" wx:for-index="__i1__" wx:key="cat_id"><view data-event-opts="{{[['tap',[['toCategory',['$0'],[[['categoryList','',pageindex],['','cat_id',category.cat_id]]]]]]]}}" class="icon" bindtap="__e"><image mode="widthFix" src="{{category.img}}" data-event-opts="{{[['load',[['categoryImgLoad',['$event']]]]]}}" bindload="__e"></image><view>{{category.title}}</view></view></block></view></swiper-item></block></swiper><view class="dots"><block wx:for="{{categoryList}}" wx:for-item="page" wx:for-index="pageindex" wx:key="pageindex"><view class="{{[(pageindex==currentPageindex)?'active':'']}}"></view></block></view></view></view><view class="goods-list"><view class="title">— 猜你喜欢 —</view><view class="product-list"><block wx:for="{{productList}}" wx:for-item="product" wx:for-index="__i2__" wx:key="id"><view data-event-opts="{{[['tap',[['toGoods',['$0','$1'],[[['productList','id',product.id,'id']],[['productList','id',product.id,'userId']]]]]]]}}" class="product" bindtap="__e"><image src="{{product.image}}"></image><view class="name">{{product.title}}</view><view class="info"><view class="price">{{"￥"+product.price}}</view><view class="collect">{{"收藏人数："+product.collect}}</view></view></view></block></view><view class="loading-text">{{loadingText}}</view></view></view>